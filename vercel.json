{
  "version": 2,
  "builds": [
    {
      "src": "apphttp.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apphttp.py"
    }
  ],
  "functions": {
    "api/**.py": {
      "maxDuration": 60  // Tăng thời gian chờ lên 60 giây
    }
  }
}
